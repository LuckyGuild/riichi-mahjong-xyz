{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["dom"],
    "jsx": "react-jsx",
    "sourceMap": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noPropertyAccessFromIndexSignature": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "noEmit": true
  },
  "include": ["./src"],
  "exclude": ["./dist"]
}
